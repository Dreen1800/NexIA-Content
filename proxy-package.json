{
    "name": "instagram-proxy-server",
    "version": "1.0.0",
    "description": "Instagram Image Proxy Server for Production",
    "main": "proxy-server.js",
    "scripts": {
        "start": "node proxy-server.js",
        "dev": "nodemon proxy-server.js",
        "pm2:start": "pm2 start proxy-server.js --name instagram-proxy",
        "pm2:stop": "pm2 stop instagram-proxy",
        "pm2:restart": "pm2 restart instagram-proxy"
    },
    "keywords": [
        "instagram",
        "proxy",
        "cors",
        "images"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "http-proxy-middleware": "^2.0.6",
        "cors": "^2.8.5",
        "node-fetch": "^2.6.7"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}